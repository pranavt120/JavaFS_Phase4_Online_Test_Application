package com.pranav.populate.utility;

import com.fasterxml.jackson.annotation.JsonProperty;

public class Tag {
	
	@JsonProperty
	private int id;
	@JsonProperty
	private String name;
	
	public Tag() {}

	public Tag(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}



}
