package com.pranav.dto;


public class TagsDto {
	
	private int id;

	private String name;
	

	public TagsDto() {}

	public TagsDto(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	

}
